server {
    server_name www.timeful.app;
    return 301 https://timeful.app$request_uri;

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/timeful.app/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/timeful.app/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = www.timeful.app) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name www.timeful.app;

    listen 80;
    return 404; # managed by Certbot


}